pkgbase = libssh2
	pkgdesc = A library implementing the SSH2 protocol as defined by Internet Drafts
	pkgver = 1.7.0
	pkgrel = 1
	url = https://libssh2.org/
	arch = i686
	arch = x86_64
	groups = libraries
	license = BSD
	makedepends = openssl-devel
	makedepends = zlib-devel
	options = !libtool
