pkgname = libssh2
	pkgdesc = Multi-protocol file transfer library (runtime)
	groups = libraries
	depends = ca-certificates
	depends = openssl
	depends = zlib

pkgname = libssh2-devel
	pkgdesc = Libssh2 headers and libraries
	groups = development
	depends = libssh2=1.7.0
	depends = openssl-devel
	depends = zlib-devel
	options = staticlibs

