pkgbase = zlib
	pkgdesc = Compression library implementing the deflate compression method found in gzip and PKZIP
	pkgver = 1.2.11
	pkgrel = 1
	url = http://www.zlib.net/
	arch = i686
	arch = x86_64
	groups = libraries
	license = custom
	makedepends = gcc
	makedepends = make
	source = https://sourceforge.net/projects/libpng/files/zlib/1.2.11/zlib-1.2.11.tar.gz
	source = 1.2.7-minizip-cygwin.patch
	source = 1.2.7-zlib-symbols.patch
	source = 1.2.11-cygwin-no-widechar.patch
	source = zlib-1.2.11-msys2.patch
