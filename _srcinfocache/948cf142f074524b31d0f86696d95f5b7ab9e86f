pkgname = glib2
	license = LGPL
	depends = libxslt
	depends = libpcre
	depends = libffi
	depends = libiconv
	depends = zlib
	optdepends = gamin: for gio fam module
	optdepends = python2: for gdbus-codegen and gtester-report
	options = !docs
	options = !emptydirs

pkgname = glib2-devel
	pkgdesc = glib2 headers and libraries
	groups = development
	depends = glib2=2.48.0
	depends = pcre-devel
	depends = libffi-devel
	depends = libiconv-devel
	depends = zlib-devel
	options = emptydirs

pkgname = glib2-docs
	pkgdesc = Documentation for glib2
	license = custom
	conflicts = gobject2-docs
	replaces = gobject2-docs
	options = docs
	options = !emptydirs

