	depends = msys2-runtime
	depends = dash
	source = rebase-4.4.2::git://sourceware.org/git/cygwin-apps/rebase.git#tag=rebase-4-4-2
	source = rebase-4.4.1-msys2.patch
	source = autorebase.bat
	source = autorebasebase1st.bat
	source = rebaseall-add-python-exts.patch
	source = pacman-rec-filename-grep
	source = allow-non-database-mode-when-__CYGWIN__-__MSYS__.patch
	source = msys2-usr.patch
