	depends = mingw-w64-x86_64-adwaita-icon-theme
	depends = mingw-w64-x86_64-gettext
	depends = mingw-w64-x86_64-gtk3
	depends = mingw-w64-x86_64-gdk-pixbuf2
	depends = mingw-w64-x86_64-gobject-introspection-runtime
	depends = mingw-w64-x86_64-gsettings-desktop-schemas
	depends = mingw-w64-x86_64-zlib
	depends = mingw-w64-x86_64-libexif
	depends = mingw-w64-x86_64-libjpeg-turbo
	depends = mingw-w64-x86_64-libpeas
	depends = mingw-w64-x86_64-librsvg
	depends = mingw-w64-x86_64-libxml2
	depends = mingw-w64-x86_64-shared-mime-info
	options = strip
	options = staticlibs
	source = https://download.gnome.org/sources/eog/3.16/eog-3.16.3.tar.xz
	source = 001-win.patch
	source = 002-relocation-support.patch
