	pkgdesc = Remote desktop client for the GNOME Desktop (mingw-w64)
	pkgver = 3.8.2
	pkgrel = 2
	url = https://www.gnome.org
	install = devhelp-x86_64.install
	arch = any
	license = GPL 2
	makedepends = mingw-w64-x86_64-gobject-introspection
	makedepends = mingw-w64-x86_64-vala
	depends = mingw-w64-x86_64-gtk3
	depends = mingw-w64-x86_64-gsettings-desktop-schemas
	depends = mingw-w64-x86_64-adwaita-icon-theme
	depends = mingw-w64-x86_64-webkitgtk3
	depends = mingw-w64-x86_64-png2ico
	depends = mingw-w64-x86_64-python2
	options = strip
	options = staticlibs
