	provides = perl-MIME-Decoder
	provides = perl-MIME-Decoder-Base64
	provides = perl-MIME-Decoder-Binary
	provides = perl-MIME-Decoder-Binhex
	provides = perl-MIME-Decoder-gzip64
	provides = perl-MIME-Decoder-nbit
	provides = perl-MIME-Decoder-Quotedprint
	provides = perl-MIME-decoder-uu
	provides = perl-MIME-Entity
	provides = perl-MIME-Field-Contdisp=5.428.0
	provides = perl-MIME-Field-Contraenc=5.428.0
	provides = perl-MIME-Field-Conttype=5.428.0
	provides = perl-MIME-Field-Paramval=5.428.0
	provides = perl-MIME-Head=5.428.0
	provides = perl-MIME-Parser
	provides = perl-MIME-Parser-Filer
	provides = perl-MIME-Parser-Reader
	provides = perl-MIME-Parser-Results
	provides = perl-MIME-WordDecoder
	provides = perl-MIME-Words=5.428.0
