	depends = libbz2
	depends = libiconv
	depends = liblzma
	depends = liblzo2
	depends = libnettle
	depends = libxml2
	depends = zlib
	options = !strip
	options = debug
	options = libtool
	source = http://libarchive.org/downloads/libarchive-3.2.2.tar.gz
