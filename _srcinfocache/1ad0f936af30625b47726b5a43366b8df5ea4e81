	pkgdesc = Brush engine used by MyPaint (git) (mingw-w64)
	pkgver = 1.3.0
	pkgrel = 1
	url = http://mypaint.org
	arch = any
	license = ISC
	makedepends = mingw-w64-x86_64-gcc
	makedepends = mingw-w64-x86_64-pkg-config
	makedepends = mingw-w64-x86_64-json-c
	makedepends = mingw-w64-x86_64-glib2
	makedepends = mingw-w64-x86_64-gobject-introspection
	makedepends = autoconf
	makedepends = automake
	makedepends = libtool
	makedepends = git
	depends = mingw-w64-x86_64-gcc-libs
	depends = mingw-w64-x86_64-glib2
	provides = mingw-w64-x86_64-libmypaint
	conflicts = mingw-w64-x86_64-libmypaint
	conflicts = mingw-w64-x86_64-mypaint<1.3.0alpha
	options = !strip
	options = debug
	source = libmypaint::git+https://github.com/mypaint/libmypaint.git
	sha256sums = SKIP

pkgname = mingw-w64-x86_64-libmypaint-git

