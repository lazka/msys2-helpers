pkgbase = sqlite
	pkgdesc = A C library that implements an SQL database engine
	pkgver = 3.10.0.0
	pkgrel = 1
	url = https://www.sqlite.org/
	arch = i686
	arch = x86_64
	license = PublicDomain
	makedepends = libreadline-devel
	makedepends = icu-devel
	makedepends = zlib-devel
	depends = libreadline
	depends = icu
	depends = zlib
	options = !libtool
	options = !emptydirs
	options = !debug
	options = strip
	source = https://www.sqlite.org/2016/sqlite-autoconf-3100000.tar.gz
	source = https://www.sqlite.org/2016/sqlite-doc-3100000.zip
	source = LICENSE
	source = sqlite-3.8.0.1-msys2.patch
	source = sqlite3-3.10.0-1.src.patch
	source = sqlite3-cygwin-msys.patch
	sha256sums = 43cc292d70711fa7580250c8a1cd7c64813a4a0a479dbd502cce5f10b5d91042
