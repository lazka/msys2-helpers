pkgname = sqlite
	pkgdesc = A C library that implements an SQL database engine
	depends = libreadline
	depends = libsqlite
	provides = sqlite3=3.10.0.0
	replaces = sqlite3

pkgname = libsqlite
	pkgdesc = Sqlite3 library
	groups = libraries

pkgname = libsqlite-devel
	pkgdesc = Sqlite3 headers and libraries
	groups = development
	depends = libsqlite=3.10.0.0
	options = !libtool
	options = !emptydirs
	options = !debug
	options = strip
	options = staticlibs

pkgname = sqlite-doc
	pkgdesc = Most of the static HTML files that comprise this website, including all of the SQL Syntax and the C/C++ interface specs and other miscellaneous documentation
	provides = sqlite3-doc=3.10.0.0
	replaces = sqlite3-doc

